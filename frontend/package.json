{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "4.7.0",
    "@react-keycloak/web": "3.4.0",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.3",
    "@testing-library/user-event": "13.5.0",
    "@types/humps": "2.0.1",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@types/react-router-dom": "5.3.3",
    "antd": "4.18.8",
    "axios": "0.26.0",
    "cors-anywhere": "0.4.4",
    "dotenv": "16.0.0",
    "env-cmd": "10.1.0",
    "humps": "2.0.1",
    "keycloak-js": "17.0.0",
    "moment": "2.29.1",
    "query-string": "7.1.1",
    "react": "17.0.2",
    "react-async": "10.0.1",
    "react-dom": "17.0.2",
    "react-ga": "3.3.0",
    "react-hotjar": "5.0.0",
    "react-joyride": "2.3.2",
    "react-router-dom": "6.2.1",
    "react-scripts": "5.0.0",
    "typescript": "4.5.5",
    "uuid": "8.3.2"
  },
  "scripts": {
    "start:local": "env-cmd -f .envs/.local/.react react-scripts start",
    "start:production": "env-cmd -f .envs/.production/.react react-scripts start",
    "build:local": "env-cmd -f .envs/.local/.react react-scripts build",
    "build:production": "env-cmd -f .envs/.production/.react react-scripts build",
    "eject": "react-scripts eject",
    "test": "CI=1 env-cmd -f .envs/.local/.react react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:watch": "env-cmd -f .envs/.local/.react react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:coverage": "CI=1 env-cmd -f .envs/.local/.react react-scripts test --coverage --env=jest-environment-jsdom-sixteen",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx,json}' --fix --max-warnings=0 --no-error-on-unmatched-pattern && tsc --noemit",
    "precommit": "eslint './src/**/*.{js,jsx,ts,tsx,json}' --max-warnings=0 --no-error-on-unmatched-pattern"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "jest-environment-jsdom-sixteen": "2.0.0",
    "msw": "0.38.1",
    "prettier": "2.5.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!src/index.tsx",
      "!**/serviceWorker.js",
      "!**/react-app-env.d.ts",
      "!**/lib/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      },
      "./src/components/App/App.tsx": {
        "lines": 100
      }
    }
  }
}
