{
  "name": "frontend",
  "version": "1.5.2",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx,json}' --fix --max-warnings=0 --no-error-on-unmatched-pattern && tsc --noemit",
    "precommit": "eslint './src/**/*.{js,jsx,ts,tsx,json}' --max-warnings=0 --no-error-on-unmatched-pattern",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "eslintIgnore": [
      "src/test/**",
      "src/**/*.test.ts",
      "src/**/*.test.tsx"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.tsx",
      "!src/test/**",
      "!src/assets",
      "!**/lib/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      },
      "./src/components/App/App.tsx": {
        "lines": 95
      }
    },
    "moduleNameMapper": {
      "react-markdown": "<rootDir>/src/test/__mocks__/ReactMarkdownMock.tsx",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|css|less)$": "<rootDir>/src/test/__mocks__/assetFileMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(t|j)sx?$": "@swc/jest"
    }
  },
  "dependencies": {
    "@ant-design/icons": "6.0.0",
    "@ctrl/tinycolor": "^4.1.0",
    "@react-keycloak/web": "3.4.0",
    "antd": "^5.25.1",
    "autoprefixer": "10.4.21",
    "axios": "1.9.0",
    "dayjs": "1.11.13",
    "dotenv": "16.5.0",
    "humps": "2.0.1",
    "jotai": "2.12.4",
    "js-pkce": "^2.0.0",
    "keycloak-js": "19.0.3",
    "lz-string": "^1.5.0",
    "prop-types": "15.8.1",
    "query-string": "7.0.0",
    "react": "18.2.0",
    "react-async": "10.0.1",
    "react-dom": "18.2.0",
    "react-ga4": "^2.1.0",
    "react-hotjar": "6.3.1",
    "react-joyride": "2.9.3",
    "react-markdown": "10.1.0",
    "react-router": "7.6.0",
    "typescript": "5.8.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@swc/core": "^1.11.24",
    "@swc/jest": "^0.2.38",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/gtag.js": "^0.0.20",
    "@types/humps": "2.0.6",
    "@types/jest": "^29.5.14",
    "@types/node": "22.15.14",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "8.33.0",
    "@typescript-eslint/parser": "8.33.0",
    "@vitejs/plugin-react": "^4.4.1",
    "cross-fetch": "^4.1.0",
    "eslint": "8.57.1",
    "eslint-config-airbnb": "18.0.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.4.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "7.2.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mock-match-media": "^0.4.3",
    "msw": "1.3.5",
    "postcss": "8.5.3",
    "prettier": "^3.5.3",
    "setimmediate": "1.0.5",
    "vite": "^6.3.6"
  }
}
