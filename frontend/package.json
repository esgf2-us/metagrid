{
  "name": "frontend",
  "version": "1.0.10-beta",
  "private": true,
  "scripts": {
    "build:local": "env-cmd -f .envs/.react react-scripts build",
    "build:production": "env-cmd -f .envs/.prod.env react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx,json}' --fix --max-warnings=0 --no-error-on-unmatched-pattern && tsc --noemit",
    "precommit": "eslint './src/**/*.{js,jsx,ts,tsx,json}' --max-warnings=0 --no-error-on-unmatched-pattern",
    "start:local": "env-cmd -f .envs/.react react-scripts start",
    "start:production": "env-cmd -f .envs/.prod.env react-scripts start",
    "test": "CI=1 env-cmd -f .envs/.react react-scripts test --runInBand",
    "test:coverage": "CI=1 env-cmd -f .envs/.react react-scripts test --coverage",
    "test:watch": "env-cmd -f .envs/.react react-scripts test --runInBand"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "eslintIgnore": [
      "src/**/*.test.ts",
      "src/**/*.test.tsx"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!src/index.tsx",
      "!src/test/**",
      "!src/assets",
      "!src/api/mock/**",
      "!**/serviceWorker.js",
      "!**/react-app-env.d.ts",
      "!**/lib/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      },
      "./src/components/App/App.tsx": {
        "lines": 100
      }
    },
    "moduleNameMapper": {
      "react-markdown": "<rootDir>/node_modules/react-markdown/react-markdown.min.js"
    }
  },
  "dependencies": {
    "@ant-design/icons": "5.0.1",
    "@babel/plugin-syntax-flow": "7.16.7",
    "@babel/plugin-transform-react-jsx": "7.17.3",
    "@react-keycloak/web": "3.4.0",
    "antd": "4.24.12",
    "autoprefixer": "10.4.14",
    "axios": "0.26.1",
    "dotenv": "8.2.0",
    "env-cmd": "10.1.0",
    "humps": "2.0.1",
    "js-pkce": "1.2.1",
    "keycloak-js": "19.0.3",
    "moment": "2.29.4",
    "prop-types": "15.8.1",
    "query-string": "7.0.0",
    "react": "18.2.0",
    "react-async": "10.0.1",
    "react-dom": "18.2.0",
    "react-hotjar": "2.2.1",
    "react-joyride": "2.5.3",
    "react-markdown": "8.0.7",
    "react-router-dom": "^6.9.0",
    "react-scripts": "5.0.1",
    "recoil": "0.7.7",
    "typescript": "4.6.3",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/dom": "9.3.1",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/humps": "2.0.2",
    "@types/jest": "28.1.8",
    "@types/node": "14.14.37",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "@types/react-router-dom": "5.3.3",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "dayjs": "1.11.7",
    "eslint": "8.45.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.33.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "msw": "0.28.1",
    "postcss": "8.4.21",
    "prettier": "2.2.1",
    "setimmediate": "1.0.5"
  }
}
